set nocompatible

color smyck
syntax enable
set tabstop=2

set number
set ruler

set encoding=utf-8

set nowrap
set shiftwidth=2
set softtabstop=2
set expandtab
" set list listchars=tab:»·,trail:·

set backspace=indent,eol,start

" Search related settings
set incsearch
set hlsearch

set noswapfile
set nobackup
